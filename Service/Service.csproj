  </ItemGroup>

  <ItemGroup>
    <Reference Include="WSMBS">
      <HintPath>Dependencies\WSMBS.dll</HintPath>
    </Reference>
    <Reference Include="WSMBT">
      <HintPath>Dependencies\WSMBT.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>
